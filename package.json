{
	"name": "wc-air-datepicker",
	"version": "0.3.0",
	"description": "Web component wrapper around air-datepicker",
	"module": "dist/esm/index.js",
	"main": "dist/index.js",
	"types": "dist/types/components.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "stencil build",
		"start": "stencil build --dev --watch --serve",
		"test": "jest",
		"test.watch": "jest --watch"
	},
	"author": "Domantas Mauruƒça <domantas.mauruca@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Dohxis/air-datepicker/issues"
	},
	"keywords": [
		"air-datepicker",
		"datepicker",
		"timepicker",
		"year picker",
		"month picker",
		"date",
		"time",
		"month",
		"year"
	],
	"dependencies": {},
	"devDependencies": {
		"@stencil/core": "^0.11.2",
		"@types/jest": "^23.3.1",
		"jest": "^23.4.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dohxis/air-datepicker.git"
	},
	"homepage": "https://github.com/Dohxis/air-datepicker",
	"jest": {
		"transform": {
			"^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json",
			"jsx"
		]
